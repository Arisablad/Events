{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "../dist/mock",
    "rootDir": ".",
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": false,
    "esModuleInterop": true,
    "typeRoots": ["../node_modules/@types"]
  },
  "include": ["./**/*.ts"]
}
